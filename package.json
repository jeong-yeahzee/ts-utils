{
  "name": "@yeahzee/ts-utils",
  "version": "1.0.6",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./index.js",
  "typings": "./index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "import": "./index.js",
    "require": "./index.cjs"
  },
  "scripts": {
    "dev": "tsup src/index.ts --watch --dts",
    "test": "vitest run",
    "build": "tsup src/index.ts --format esm,cjs --dts --minify",
    "prepublishOnly": "pnpm test && pnpm build"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
